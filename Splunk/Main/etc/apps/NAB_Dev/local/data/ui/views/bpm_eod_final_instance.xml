<form stylesheet="single_decorations.css, bpm_legend.css">
  <label>BPM EOD Final Instance</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="text" token="businessKey" searchWhenChanged="true">
      <label>Business Key</label>
    </input>
  </fieldset>
  <search id="baseQuery">
    <query>index="camunda" source=BpmActivityEODFinal BusinessKey=$businessKey$ | eval value = if (State=2, "severe", if(EndTime == "null", "high", "low")) | table value Name</query>
    <earliest>0</earliest>
    <latest></latest>
  </search>
  <row>
    <panel>
      <html src="bpm_legend.html">
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <single>
        <title>Start End of Day Final</title>
        <search base="baseQuery">
          <query>| Search Name="Start Event"</query>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
        <option name="classField">value</option>
        <option name="field">value</option>
        <option name="additionalClass">icon-only</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Set End of Day Reference Data</title>
        <search base="baseQuery">
          <query>| search Name="Set EOD Reference"</query>
