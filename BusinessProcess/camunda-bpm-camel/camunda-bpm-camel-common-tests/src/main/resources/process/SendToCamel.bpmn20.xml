      <outgoing>SequenceFlow_2</outgoing>
    </serviceTask>
    <sequenceFlow id="SequenceFlow_2" name="" sourceRef="logProcessVariableServiceTask" targetRef="end"/>
    <endEvent id="end">
      <incoming>SequenceFlow_2</incoming>
    </endEvent>
  </process>
